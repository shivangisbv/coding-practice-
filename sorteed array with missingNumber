//Find the sortted array with missing number without in built function
let  originalArr = [1,3,5,7,9];
let missingNum =[];
for(let i=0; i< originalArr.length -1 ; i++){
  let current = originalArr[i];
  let next = originalArr[i+1];
  for(let j= current +1 ; j< next ; j++){
    missingNum.push(j);
  }
}
console.log(missingNum)



//using function 


function findmisssingNumber(originalArr){
  let missingNum =[];
  for(let i=0; i< originalArr.length -1 ; i++){
    let current = originalArr[i];
    let next = originalArr[i+1];
      for(let j= current +1 ; j< next ; j++){
        missingNum.push(j);
      }
  }
return missingNum;
}

let result = findmisssingNumber([1,3,5,7,9]);
console.log(result);
